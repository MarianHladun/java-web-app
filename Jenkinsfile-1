pipeline {
  
agent {
  label 'master'
}
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'Sonar-DLab') { 
          sh 'ip a'
          sh 'java -version'
          sh 'mvn -version'
          sh './mvnw clean sonar:sonar'
        }
      }
    }
  }
}
